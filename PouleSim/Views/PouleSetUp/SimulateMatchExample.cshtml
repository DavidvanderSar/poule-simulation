@{
    ViewData["Title"] = "Simulated Match result";
}

<h2>Welcome</h2>

<p>In total, there were @ViewData["HomeWins"] home wins, @ViewData["Draws"] draws and @ViewData["AwayWins"] away wins.</p>

<ul>
    @for (int i = 0; i < (int)ViewData["Times"]; i++)
    {
        <li>@ViewData[$"Match{i}"]</li>
    }
</ul>